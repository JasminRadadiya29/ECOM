{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "build/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://ecomus-api-sepia.vercel.app/"
  }
}
